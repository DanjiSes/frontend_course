ckground: lighten($color, 20);\r\n    }\r\n  }\r\n\r\n  .btn-outline-#{$name} {\r\n    color: $color;\r\n    background: none;\r\n    border: 1px solid $color;\r\n\r\n    &:hover {\r\n      color: #fff;\r\n      background: $color;\r\n    }\r\n\r\n    &:active {\r\n      background: darken($color, 15);\r\n    }\r\n  }\r\n}\r\n\r\n.btn-lg {\r\n  border-radius: 7px;\r\n  padding: 18px 30px;\r\n  font-size: 18px;\r\n}\r\n\r\n.btn-xl {\r\n  border-radius: 10px;\r\n  padding: 20px 36px;\r\n  font-size: 20px;\r\n  font-weight: 900;\r\n}\r\n\r\n.btn-pill {\r\n  border-radius: 300px;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"9b65bd08050677d62fff8641b100ecbe","cacheData":{"env":{}}}